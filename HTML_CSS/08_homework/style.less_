@import url('https://fonts.googleapis.com/css?family=Montserrat:100,300,700');
@widthAll =1366px;
.body {
  font-family: 'Montserrat', sans-serif;
}

* {
  margin: 0;
  padding: 0;
  background-repeat: no-repeat;
}

.container {
  width: @widthAll;
  margin: 0 auto;
}

.navbar {
  height: 90px;
}

.navbar {
  .logo {
    float: left;
  }
  .menu {
    float: right;
  }
}